{
  "keybindings": {
    "DirList": {
      "<q>": "Quit", // Quit the application
      "<Ctrl-z>": "Suspend", // Suspend the application
      "<j>": "SelectNext",
      "<k>": "SelectPrev",
      "<Shift-g>": "SelectLast",
      "<g><g>": "SelectFirst",
      "<h>": "MoveUpDir",
      "<l>": "MoveDownDir",
      "<Enter>": "MoveDownDir",
    },
  }
}
